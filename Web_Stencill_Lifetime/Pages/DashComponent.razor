@using Web_Stencill_Lifetime.Data;
@using Web_Stencill_Lifetime.Model;

<div class="girdContent">
	<div class="Griditem">
		<h1>Dashboard</h1>
	</div>
	<br />
	<br />
     	<div class="Griditem">
		<div class="childgird">
			  <div class="CardAlljig">
				<div class="girdItemAlljig"><h2>Stencil in Line</h2></div>
				<hr />
				<div class="girdItemAlljig">
					<div>
						<svg xmlns="http://www.w3.org/2000/svg" width="80" height="100" fill="currentColor" class="bi bi-pin-map" viewBox="0 0 16 16">
						  <path fill-rule="evenodd" d="M0 0h1v15h15v1H0V0Zm10 3.5a.5.5 0 0 1 .5-.5h4a.5.5 0 0 1 .5.5v4a.5.5 0 0 1-1 0V4.9l-3.613 4.417a.5.5 0 0 1-.74.037L7.06 6.767l-3.656 5.027a.5.5 0 0 1-.808-.588l4-5.5a.5.5 0 0 1 .758-.06l2.609 2.61L13.445 4H10.5a.5.5 0 0 1-.5-.5Z"/>
					    </svg>					
						</div>
					@if (ob[0].TotalInline != "")
					{
					<div><h1>@ob[0].TotalInline</h1></div>	
					}
					
				</div>
			  </div>
		</div>
		<div class="childgird">
			<div class="CardAlljig">
				<div class="girdItemAlljig"><h2>Stencil all</h2></div>
				<hr />
				<div class="girdItemAlljig">
					<div>
						<svg xmlns="http://www.w3.org/2000/svg" width="80" height="100" fill="currentColor" class="bi bi-pin-map" viewBox="0 0 16 16">
						  <path d="M13.5 1a1 1 0 0 1 1 1v12a1 1 0 0 1-1 1h-11a1 1 0 0 1-1-1V2a1 1 0 0 1 1-1h11zm-11-1a2 2 0 0 0-2 2v12a2 2 0 0 0 2 2h11a2 2 0 0 0 2-2V2a2 2 0 0 0-2-2h-11z"/>
						  <path d="M6.5 3a1 1 0 0 1 1-1h1a1 1 0 0 1 1 1v3a1 1 0 0 1-1 1h-1a1 1 0 0 1-1-1V3zm-4 0a1 1 0 0 1 1-1h1a1 1 0 0 1 1 1v7a1 1 0 0 1-1 1h-1a1 1 0 0 1-1-1V3zm8 0a1 1 0 0 1 1-1h1a1 1 0 0 1 1 1v10a1 1 0 0 1-1 1h-1a1 1 0 0 1-1-1V3z"/>					  </svg>
					</div>
					@if (ob[0].TotalStencil != "")
					{
					  <div><h1>@ob[0].TotalStencil</h1></div>	
					}
					
				</div>
			</div>
		</div>
	</div>
</div>
@code {

	 List<ObDashboard> ob = new List<ObDashboard>();
	GetData db = new GetData();
	protected override async Task OnInitializedAsync()
	{
	  await	GetDash();
	}
	private bool Render { get; set; }
	private async Task GetDash()
	{
		try
		{
			ob = await db.GetCountUseinLine();
		}	catch{}
	}
}
